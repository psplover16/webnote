    "@vitejs/plugin-vue": "^4.5.2",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^30.0.3",
    "electron-builder": "^24.13.3",
    "sass": "^1.70.0",
    "sass-loader": "^14.1.0",
    "vite": "^5.0.8",
    "wait-on": "^7.2.0"
